{
	"name": "webpack-vue-boilerplate",
	"version": "1.2.0",
	"description": "Simple boilerplate for building Vue app with Webpack.",
	"main": "src/main.js",
	"scripts": {
		"dev": "npx cross-env NODE_ENV=development npx webpack-dev-server --hot --open",
		"build": "npx cross-env NODE_ENV=production npx webpack --hide-modules --progress"
